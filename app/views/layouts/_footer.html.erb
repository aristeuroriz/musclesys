<div class="conteiner">
	<div class="row">

		<div class="col-md-4">


		</div>

		<div class="col-md-4" id="p2"> 

			<br>


			<%= link_to (image_tag "google-256.png",class: "social-icon" ), 'http://musclesys.com.br',  :target => "_blank"%> 
			<%= link_to (image_tag "facebook-256.png",class: "social-icon"), 'https://www.facebook.com/pages/MuscleSys/1019521451407294',  :target => "_blank" %> 
			<%= link_to (image_tag "twitter-256.png",class: "social-icon"), 'https://twitter.com/Muscle_Sys',  :target => "_blank" %> 

			<%= link_to (image_tag "email-256.png",class: "social-icon"), '#contato',:data => { :toggle =>"modal" } %>

			<h6>© 2014 MuscleSys. Todos os direitos reservados. <br> Desenvolvido por  <%= link_to('Arneti Tecnologia', 'http://arneti.com.br', :target => '_blank', class: "arneti"  )%> </h6>

			<h6>Brasil / Goiás</h6>
			<%=image_tag "brasil_goias.jpg", class: "band" %>


		</div>

		<div class="col-md-4">



		</div>
	</div>

	<div id="contato" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="contatoLabel" aria-hidden="true">
		<div class="modal-dialog">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
					<h3 id="contatoLabel">Gostaríamos muito de ouvir de você</h3>
				</div>
				<div class="modal-body">
					<form class="form-horizontal col-sm-12">

						<div class="form-group"><label>Nome</label><input class="form-control required" placeholder="Seu nome" data-placement="top" data-trigger="manual" data-content="Deve ter pelo menos 3 caracteres e deve conter apenas letras." type="text"></div>

						<div class="form-group"><label>E-Mail</label><input class="form-control email" placeholder="email@you.com (para que possamos entrar em contato)" data-placement="top" data-trigger="manual" data-content="Deve ser um endereço de e-mail válido (user@gmail.com)" type="text"></div>

						<div class="form-group"><label>Mensagem</label><textarea class="form-control" placeholder="Sua mensagem aqui.." data-placement="top" data-trigger="manual"></textarea></div>
						
					
						<div class="form-group"><button type="submit" class="btn btn-lg btn-success center-block">Enviar</button> <p class="help-block pull-left text-danger hide" id="form-error">&nbsp; Desta forma não é válido. </p></div>
					</form>
				</div>
				<div class="modal-footer">
					<button class="btn" data-dismiss="modal" aria-hidden="true">Cancelar</button>
				</div>
			</div>
		</div>
	</div>

</div>

